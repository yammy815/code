<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 指向dao接口，做dao实现 -->
<mapper namespace="com.zs.pms.dao.DeptDao">
	<!-- 根据id查询 -->
	<select id="queryById" parameterType="int" resultType="TDepPo">
		select *
		from tdep where pid=#{pid}
	</select>
</mapper>
